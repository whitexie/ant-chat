{
  "name": "web",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --mode production",
    "build:preview": "tsc -b && vite build --mode preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "update:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky",
    "type-check": "tsc -b",
    "test-coverage": "vitest run --coverage",
    "test-codecov": "vitest --reporter=junit --outputFile=test-report.junit.xml",
    "bundle-analyzer": "pnpm run build && npx @codecov/bundle-analyzer ./dist --bundle-name=ant-chat --upload-token=$BUNDLE_ANALYZER_UPLOAD_TOKEN"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@ant-design/x": "^1.0.5",
    "ahooks": "^3.8.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "immer": "^10.1.1",
    "js-base64": "^3.7.7",
    "lodash-es": "^4.17.21",
    "mermaid": "^11.5.0",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "unist-util-visit": "^5.0.0",
    "use-immer": "^0.11.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@ant-chat/shared": "workspace:*",
    "@ant-design/icons": "^5.5.2",
    "@codecov/bundle-analyzer": "^1.9.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.22.7",
    "babel-plugin-react-compiler": "19.1.0-rc.1",
    "code-inspector-plugin": "^0.20.1",
    "fake-indexeddb": "^6.0.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "sass": "^1.85.0",
    "unocss": "^0.65.3",
    "vite-bundle-analyzer": "^0.16.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-magic-preloader": "^1.0.5",
    "vite-plugin-svgr": "^4.3.0"
  }
}
